{
  "name": "@jose-andres/create-routing-react",
  "version": "1.0.15",
  "description": "A CLI to create a Vite's React project with routing using react-router-dom",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "type": "module",
  "bin": {
    "@jose-andres/create-routing-react": "dist/index.js",
    "create-routing-react": "dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm --clean --minify",
    "release": "pnpm run build && changeset publish",
    "lint": "tsc"
  },
  "engines": {
    "npm": ">=8.0.0",
    "node": ">=16.0.0"
  },
  "keywords": [
    "cli",
    "create-routing-react",
    "react"
  ],
  "author": "Jose Andrés Mejías",
  "license": "MIT",
  "files": [
    "src/",
    "project-skeleton/"
  ],
  "dependencies": {
    "@changesets/cli": "^2.26.1",
    "esm": "^3.2.25",
    "ncp": "^2.0.0"
  },
  "devDependencies": {
    "@types/ncp": "^2.0.5",
    "@types/node": "^18.15.5",
    "tsup": "^6.7.0",
    "typescript": "^5.0.2"
  }
}
